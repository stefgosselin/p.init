{
    "name": "stefgosselin/p.init",
    "description": "P.init is a simple tool that helps install a few popular frameworks quickly.",
    "type": "application",
    "keywords": ["developpment", "install"],
    "license": "MIT",
    "time": "2014-11-30",
    "require": {
        "codeception/codeception": "2.0.8",
        "phpmd/phpmd": "2.1.3",
        "phpunit/php-invoker": "1.1.3",
        "phpunit/php-code-coverage": "2.0.11",
        "phpunit/phpunit": "4.3.5",
        "sebastian/phpcpd": "2.0.1",
        "phpdocumentor/phpdocumentor": "2.8.1",
        "phploc/phploc": "2.0.6",
        "phing/phing": "2.9.0",
        "symfony/symfony": "2.5.7"
    },
   "scripts": {
        "pre-install-cmd": "MyVendor\\MyClass::doSomething",
        "post-install-cmd": [
            "MyVendor\\MyClass::warmCache",
            "phpunit -c app/"
        ]
    },


    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink"
    },
    "authors": [
        {
            "name": "Stef",
            "email": "stef@3wsimple.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "": "src/" }
    }
}
